<div id="wrapper">
    <h1>Relix News</h1>

    <div id="articles">

        {{#each data}}
        <div>
            <p id="title"> {{this.title}}</p>
            <a href="{{this.link}}"> {{this.link}}</a>
            <p>{{this.summary}}</p>
            <button id="comment" btn btn-primary>Comment</button>
        </div>

        {{/each}}
    </div>
    <div id="notes">
        <h2>
            <i>Relix Comments</i>
        </h2>
    </div>
</div>